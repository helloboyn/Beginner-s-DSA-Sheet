class Solution{
	public:
	string digitsNum(int N)
	{
	    int num = N;
	    int cnt = 0;
	    while(num > 9){
	        num = num -9;
	        cnt++;
	    }
	    string str = to_string(num);
	    for(int i =0;i<cnt;i++)
	        str = str + '9';
	    for(int i =0;i<N;i++)
	        str = str + '0';
	    return str;
	}
};
